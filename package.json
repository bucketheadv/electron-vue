{
  "name": "electron-vue",
  "version": "0.0.1",
  "description": "electron vue application.",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --watch --colors --config webpack-config/webpack.config.development.js",
    "build": "node_modules/webpack/bin/webpack.js --progress --colors --config webpack-config/webpack.config.production.js",
    "start": "NODE_ENV=development electron .",
    "prod": "npm run build && NODE_ENV=production electron .",
    "pack": "npm run build && gulp pack",
    "eslint": "node_modules/eslint/bin/eslint.js --ext .vue src"
  },
  "keywords": [
    "vue",
    "electron"
  ],
  "author": "sven",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "pre-commit": [
    "eslint"
  ],
  "dependencies": {
    "electron": "^1.4.6",
    "vue": "^2.0.5",
    "vue-carbon": "^0.5.1",
    "vue-router": "^2.0.1",
    "vuex": "^2.0.0",
    "vux": "^0.1.3"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.25.0",
    "electron-packager": "^8.2.0",
    "eslint": "^3.10.0",
    "eslint-config-vue": "^2.0.1",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-vue": "^1.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "^2.24.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue-loader": "^9.8.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^0.15.0",
    "webpack-validator": "^2.2.9"
  }
}
